//Đề bài yêu cầu viết chương trình đếm xem trong đoạn [a -> b] có bao nhiêu số nguyên tố và tất cả các chữ số đó cũng là số nguyên tố
//ví dụ ta cho chạy a đến b từ 10 đến 30 thì các số nguyên tố sẽ là : 11, 13, 17, 19, 21, 23, 27, 29 => sau đó ta check các số nguyên tố đó nếu tất cả các chữ số đều là số nguyên tố thì thõa mãn điều kiện
//trong ví dụ trên những số thõa mãn sẽ là : 23, 27 (2 số)
//code
#include <stdio.h>
#include <math.h>
int max(int, int);
int check(int);
void serve(int, int);
//
int max(int a, int b) {
	return a > b ? a : b;
}
int check(int x) {
	for(int i = 2; i<=sqrt(x); i++) {
		if(x % i == 0) return 0;
	}
	return x > 1;
}
void serve(int l, int r) {
	int prime[r-l+1];
	for(int i = 0; i <= r-l; i++) {
		prime[i] = 1;
	}
	for(int i = 2; i <= sqrt(r); i++) {
		for(int j = max(i*i, (l+i-1)/i*i); j<=r; j+=i) {
			prime[j-l] = 0;
		}
	}
	int cnt = 0;
	for(int i = l; i<=r; i++) {
		if(prime[i-l]) {
			int n = i;
			int sum = 0;
			while(n) {
				int tmp = n%10;
				if(check(tmp)) sum++;
				else {
					sum = 0;
					break;
				}
				n/=10;
			}
			if(sum) cnt++;
		}
	}
	printf("%d\n", cnt);
}
int main () {
	int t;
	scanf("%d",&t);
	while(t--) {
		int a, b;
		scanf("%d%d",&a,&b);
		serve(a, b);
	}
	
	return 0;
}
